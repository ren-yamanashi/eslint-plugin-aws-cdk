{
  "name": "eslint-plugin-awscdk",
  "version": "4.0.2",
  "description": "eslint plugin for AWS CDK projects",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "sh scripts/build.sh",
    "test": "vitest --run",
    "lint": "eslint --fix --config eslint.config.js",
    "check": "tsc --noEmit",
    "pack": "pnpm run build && npm pack",
    "docs": "pnpm run --filter=@eslint-plugin-awscdk/docs",
    "example:flat-config": "pnpm run --filter=@eslint-plugin-awscdk/example-flat-config",
    "example:classic-config": "pnpm run --filter=@eslint-plugin-awscdk/example-classic-config"
  },
  "devDependencies": {
    "@eslint/js": "catalog:core",
    "@secretlint/secretlint-rule-preset-recommend": "catalog:lint",
    "@types/node": "catalog:other",
    "@typescript-eslint/rule-tester": "catalog:core",
    "eslint": "catalog:core",
    "eslint-plugin-import": "catalog:lint",
    "pkgroll": "catalog:build",
    "secretlint": "catalog:lint",
    "typescript": "catalog:other",
    "typescript-eslint": "catalog:core",
    "vitest": "catalog:test"
  },
  "dependencies": {
    "@typescript-eslint/parser": "catalog:core",
    "@typescript-eslint/utils": "catalog:core"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "packageManager": "pnpm@10.10.0",
  "files": [
    "dist",
    "src",
    "!src/__tests__",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "eslint",
    "typescript",
    "aws",
    "cdk"
  ],
  "homepage": "https://eslint-plugin-awscdk.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ren-yamanashi/eslint-plugin-awscdk.git"
  },
  "author": {
    "name": "ren-yamanashi"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
